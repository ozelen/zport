<%= simple_form_for(@project) do |f| %>
    <%= f.input :name %>
    <%= f.association :customer %>
    <%= f.association :provider %>
    <%= f.input :description, as: :text %>
    <%= f.input :team %>
    <%= f.input :started, as: :string, input_html: { class: 'datepicker' } %>
    <%= f.input :finished, as: :string, input_html: { class: 'datepicker' } %>
    <%= f.button :submit, class: 'col-lg-offset-2 btn-default' %>
<% end %>